[alias]
    aa = add .
    ad = add
    ap = add -p
    ba = branch -a
    bd = branch -d
    bD = branch -D
    br = branch
    ca = commit -a
    cb = checkout -b
    ch = checkout
    cl = clone
    cm = commit
    di = diff
    ds = diff --staged
    i = init
    ib = init --bare
    lg = log
    lo = log --oneline
    mf = merge --ff-only
    mg = merge --no-ff
    pa = push --all
    pd = push -d
    ph = push
    pu = push -u
    pl = pull
    ra = remote add
    rc = rebase --continue
    re = rebase
    ri = rebase -i
    rv = remove -v
    sh = stash
    sp = stash pop
    st = status
[apply]
    whitespace = nowarn
[core]
    editor = vim
[credential]
    helper = /usr/lib/git-core/git-credential-libsecret
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[init]
    templatedir = ~/.config/git/templates
[pull]
    ff = only
[push]
    default = simple
